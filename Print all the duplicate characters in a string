//User function Template for Java
class Solution 
{ 
    int sameChar(String A, String B) 
    {
        // code here  97 -65 = 32
        int i=0;
        int count =0;
        while(i!=A.length() || i !=B.length()){
            if((A.charAt(i)==B.charAt(i)) || A.charAt(i) == B.charAt(i)- 32 || B.charAt(i)== A.charAt(i)-32){
                count ++;
            }
            i++;
        }
        return count;
    }
} 
